%name Direct Mapped Writeback
%desc Direct Mapped Writeback Data Cache
%provides hasim_dcache 

%attributes hasim caches

%public  dcache-direct-mapped-writeback.bsv 
%private dcache-direct-mapped-writeback.dic

%param --dynamic DCACHE_ALWAYS_HIT  0     "0 for simulate cache, 1 for always hit"

%param DCACHE_LINE_BITS    5  "Number of block offset bits"
%param DCACHE_IDX_BITS     10 "Number of index bits"
%param DCACHE_MISS_PENALTY 10 "Miss penalty cycles (0 not a valid entry)"
